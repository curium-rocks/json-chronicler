{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["development"],
  "labels": ["dependencies"],
  "ignoreUnstable": false,
  "packageRules": [
    {
      "matchUpdateTypes": ["major","minor", "patch", "pin", "digest", "lockFileMaintenance", "bump"],
      "automerge": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    }
  ]
}
